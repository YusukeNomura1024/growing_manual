<main>
  <div class="main__content main__content_w500 panel notifications">
    <h2 class="title--center">通知一覧</h2>

    <!--自分のマニュアルに対する通知は表示しない-->
    <% notifications = @notifications.where.not(visitor_id: current_user.id) %>
    <% if notifications.exists? %>

      <%= render notifications %>
      <%= paginate notifications %>
    <% else %>
      <p>通知はありません</p>
    <% end %>
  </div>
</main>
